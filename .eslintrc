{
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
      "rules": {
    // disable requiring trailing commas because it might be nice to revert to
    // being JSON at some point, and I don't want to make big changes now.
    "comma-dangle": 0
  },
    "extends": "airbnb",
    "ecmaFeatures": {
        "modules": true,
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
}
